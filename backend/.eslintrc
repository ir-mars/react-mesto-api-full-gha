{
  "extends": ["airbnb-base", "plugin:prettier/recommended"],
  "parserOptions": {
    "ecmaVersion": 2021
  },
  "rules": {
    "no-underscore-dangle": [
      "error",
      {
        "allow": ["_id"]
      }
    ],
    "import/no-extraneous-dependencies": "off",
    "no-console": "off",
    "no-unused-expressions": "off"
  }
}
